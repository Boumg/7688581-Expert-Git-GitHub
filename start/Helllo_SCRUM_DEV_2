#include <iostream>
#include <vector>

int main() {
    std::vector<std::string> scrumDevelopers = {"Alice", "Bob", "Charlie", "David"};

    std::cout << "Salutations aux dÃ©veloppeurs SCRUM :\n";

    for(const auto& developer : scrumDevelopers) {
        std::cout << "Bonjour " << developer << "!\n";
    }

    return 0;
}
